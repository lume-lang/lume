    pub fn external <(self, other: Int8) -> Boolean
    pub fn external <=(self, other: Int8) -> Boolean
    pub fn external >(self, other: Int8) -> Boolean
    pub fn external >=(self, other: Int8) -> Boolean

    pub fn external +(self, other: Int8) -> Int8
    pub fn external -(self, other: Int8) -> Int8
    pub fn external *(self, other: Int8) -> Int8
    pub fn external /(self, other: Int8) -> Int8
}

